  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'mqttweb'",
      "cargo": {
        "args": [
          "build",
          "--bin=mqttweb",
          "--package=mqttweb"
        ],
        "filter": {
          "name": "mqttweb",
          "kind": "bin"
        }
      },
      "args": ["serve"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'mqttweb'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=mqttweb",
          "--package=mqttweb"
        ],
        "filter": {
          "name": "mqttweb",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'duke'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=duke"
        ],
        "filter": {
          "name": "duke",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
